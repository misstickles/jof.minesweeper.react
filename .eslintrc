{
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jquery": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
  }
  },
  "rules": {
    //   "quotes": 0,
    //   "no-trailing-spaces": 0,
    //   "eol-last": 0,
    "no-unused-vars": 1,
    //   "no-underscore-dangle": 0,
    //   "no-alert": 0,
    //   "no-lone-blocks": 0
    "no-var": 1
  },
  "globals": {
    "jQuery": true,
    "$": true
  }
}